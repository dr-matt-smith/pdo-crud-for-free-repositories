{
    "name": "mattsmithdev/pdo-crud-for-free-repositories",
    "type": "library",
    "description": "make it really simply to do db crud with pdo",
    "keywords": [
        "mattsmithdev",
        "pdo-crud-for-free-repositories"
    ],
    "homepage": "https://github.com/dr-matt-smith/pdo-crud-for-free-repositories",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Smith",
            "email": "dr_matt_smith@me.com",
            "homepage": "http://mattsmithdev.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Mattsmithdev\\PdoCrudRepo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mattsmithdev\\PdoCrudRepoTest\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
